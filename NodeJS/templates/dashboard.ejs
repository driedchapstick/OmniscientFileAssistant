    <main>
      <div id="recent">
        <h2 id="recentHeader">Recently Scanned Files</h2>
        <table id="recentTable" style="width: 80%">
          <tr>
            <th>File Name</th>
            <th>File Path</th>
            <th>File Extension</th>
            <th>Computer Name</th>
            <th>File Creator</th>
            <th>Date Created</th>
            <th>Date Modified</th>
            <th>File Size</th>
          </tr>
          <% for(var i=0; i < data.length; i++) { %> <%= data[i].id %>
          <tr>
            <td><%= data[i].FileName %></td>
            <td><%= data[i].FilePath %></td>
            <td><%= data[i].FileExt %></td>
            <td><%= data[i].CompName %></td>
            <td><%= data[i].FileCreator %></td>
            <td><%= data[i].FileCreated %></td>
            <td><%= data[i].FileModified %></td>
            <td><%= data[i].FileSize %></td>
          </tr>
          <% } %>
        </table>
      </div>
      <div id="schedule">
        <h2 id="scheduleHeader">Scanning Schedule</h2>
        <table id="scheduleTable" style="width: 80%">
          <tr>
            <th>Schedule Name</th>
            <th>Base Time</th>
            <th>Interval</th>
            <th>Number of Computers</th>
          </tr>
